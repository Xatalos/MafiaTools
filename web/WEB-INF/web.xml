<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>YhteydenTestaaminen</servlet-name>
        <servlet-class>YhteydenTestaaminen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Yhteys</servlet-name>
        <servlet-class>Yhteys</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestiServlet</servlet-name>
        <servlet-class>TestiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListaServlet</servlet-name>
        <servlet-class>Servlets.ListaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>YhteydenTestaus</servlet-name>
        <servlet-class>Servlets.YhteydenTestaus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BaseServlet</servlet-name>
        <servlet-class>Servlets.BaseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>YhteydenTestaaminen</servlet-name>
        <url-pattern>/YhteydenTestaaminen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Yhteys</servlet-name>
        <url-pattern>/Yhteys</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestiServlet</servlet-name>
        <url-pattern>/TestiServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaServlet</servlet-name>
        <url-pattern>/Listaustesti</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>YhteydenTestaus</servlet-name>
        <url-pattern>/YhteydenTestaus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BaseServlet</servlet-name>
        <url-pattern>/BaseServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
